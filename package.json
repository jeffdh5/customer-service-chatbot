    "prisma:seed": "ts-node src/prisma/seed.ts",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@genkit-ai/evaluator": "^0.9.9",
    "@genkit-ai/google-cloud": "^0.9.9",
    "@genkit-ai/googleai": "^0.9.9",
    "@genkit-ai/vertexai": "^0.9.9",
    "@prisma/client": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "genkit": "^0.9.9",
    "genkitx-openai": "^0.10.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "genkit-cli": "^0.9.9",
    "nodemon": "^3.1.4",
    "prisma": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
